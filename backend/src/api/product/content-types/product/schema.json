{
    "kind": "collectionType",
    "collectionName": "products",
    "info": {
        "singularName": "product",
        "pluralName": "products",
        "displayName": "Product",
        "description": "Productos Havaianas"
    },
    "options": {
        "draftAndPublish": true
    },
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "slug": {
            "type": "uid",
            "targetField": "name"
        },
        "description": {
            "type": "richtext"
        },
        "price": {
            "type": "decimal"
        },
        "images": {
            "type": "media",
            "multiple": true,
            "allowedTypes": [
                "images"
            ]
        },
        "features": {
            "type": "json"
        },
        "colors": {
            "type": "json"
        },
        "category": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::category.category"
        },
        "featured": {
            "type": "boolean",
            "default": false
        },
        "featuredOrder": {
            "type": "integer"
        },
        "collectionLabel": {
            "type": "string"
        }
    }
}